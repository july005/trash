<div class="jumbotron">
	<p id="notice"><%= notice %></p>

	<%= image_tag @pin.image.url(:medium) %>
	
	<p>
	  <strong>Description:</strong>
	  <%= @pin.description %>
	</p>

	<% if @pin.user == current_user %>
	<%= link_to 'Edit this page', edit_pin_path(@pin) %> |
	<%end%>
	<%= link_to 'Back', pins_path %>
	
</div>
